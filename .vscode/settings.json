{
    "java.configuration.updateBuildConfiguration": "automatic",
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "java.test.config": [
        {
            "name": "owlcms logging",
            "workingDirectory": "${workspaceFolder}",
            "vmargs": [ "-ea", "-Dlogback.configurationFile=logback-console.xml" ],
            "env": { "key": "value" },
        }
    ]
}