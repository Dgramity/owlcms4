pipeline {
    agent any
    tools { 
        maven '362' 
        jdk 'JDK8' 
    }
    stages {
        stage('Build owlcms') {
            steps {
                dir('owlcms') {
                    bat 'mvn -B clean package'
                }
                dir('publicresults') {
                    bat 'mvn -B clean compile'
                }
            }
        }
    }
}