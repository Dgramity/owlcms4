java -javaagent:target/heroku-javaagent-2.0.jar=stdout=true,lxmem=true -Dlogback.configurationFile=logback-console.xml -Dport=$PORT -DdevMode=true -DresetMode=true -DmemoryMode=false -Xmx400m -Xms400m -XX:MaxMetaspaceSize=80m -Dlogback.configurationFile=logback-cloud.xml -XX:+UseG1GC -XX:+UnlockExperimentalVMOptions -XX:+UseCGroupMemoryLimitForHeap -cp target/owlcms.jar app.owlcms.Main