java -javaagent:target/heroku-javaagent-2.0.jar=stdout=true,lxmem=true -Dlogback.configurationFile=logback-console.xml -Dport=$PORT -DdevMode=true -DresetMode=true -DmemoryMode=false -Xmx512m -Xms128m -XX:+UseG1GC -XX:GCTimeRatio=39 -XX:MinHeapFreeRatio=20 -XX:MaxHeapFreeRatio=30 -XX:MaxMetaspaceSize=80m -XX:+UnlockExperimentalVMOptions -XX:+UseCGroupMemoryLimitForHeap  -Dlogback.configurationFile=logback-cloud.xml -XX:+UseG1GC -XX:+UnlockExperimentalVMOptions -XX:+UseCGroupMemoryLimitForHeap -cp target/owlcms.jar app.owlcms.Main