        <!-- export format -->
        <section startRow="0" endRow="6">
            <mapping cell="F1">competition.competitionName</mapping>
            <mapping cell="F3">competition.competitionSite</mapping>
            <mapping cell="F4">competition.competitionOrganizer</mapping>
            <mapping cell="M1">competition.competitionDate</mapping>
            <mapping cell="F2">competition.competitionCity</mapping>
        </section>
		<!-- numbers are 0 based: Excel cell number is one more -->
        <!-- group is read first because it is created if missing  -->
		<loop startRow="7" endRow="7" items="athletes"
			var="a" varType="app.owlcms.spreadsheet.RAthlete">
			<section startRow="7" endRow="7">
                <mapping row="7" col="11">a.group</mapping>
				<mapping row="7" col="0">a.membership</mapping>
				<mapping row="7" col="1">a.lotNumber</mapping>
				<mapping row="7" col="2">a.lastName</mapping>
				<mapping row="7" col="3">a.firstName</mapping>
                <mapping row="7" col="4">a.team</mapping>
                <mapping row="7" col="5">a.fullBirthDate</mapping>
				<mapping row="7" col="6">a.gender</mapping>
				<mapping row="7" col="7">a.category</mapping>
                <mapping row="7" col="8">a.bodyWeight</mapping>
                <mapping row="7" col="9">a.snatch1Declaration</mapping>
                <mapping row="7" col="10">a.cleanJerk1Declaration</mapping>
				<mapping row="7" col="12">a.qualifyingTotal</mapping>
			</section>
			<loopbreakcondition>
				<rowcheck offset="0">
					<cellcheck offset="2"></cellcheck>
				</rowcheck>
			</loopbreakcondition>
		</loop>
	</worksheet>
</workbook>